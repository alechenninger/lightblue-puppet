{
  "web" => {
    "connector" => {
       "<%=name%>" => {

<% if defined? ssl %>
	"configuration" => { "<%=ssl%>" => { 
	  <% if defined? password %>
	     "password" => "<%=password%>",
	  <% end %>
	   <% if defined? certificate_key_file %>
	     "certificate-key-file" => "<%=certificate_key_file%>",
	  <% end %>
	   <% if defined? key_alias %>
	     "key-alias" => "<%=key_alias%>",
	  <% end %>
	  <% if defined? ca_certificate_file %>
	     "ca-certificate-file" => "<%=ca_certificate_file%>",
	  <% end %>
	  <% if defined? ca_certificate_password %>
	     "ca-certificate-password" => "<%=ca_certificate_password%>",
	  <% end %>
	}},
	
<% end %>

<% if defined? enable_lookups %>
                "enable-lookups" => "<%=enable_lookups%>",
<% end %>
<% if enabled %>
                "enabled" => true,
<% end %>
<% if defined? executor %>
                "executor" => "<%=executor%>",
<% end %>
<% if defined? max_connections %>
                "max-connections" => <%=max_connections%>,
<% end %>
<% if defined? max_post_size %>
                "max-post-size" => <%=max_post_size%>,
<% end %>
<% if defined? max_save_post_size %>
                "max-save-post-size" => <%=max_save_post_size%>,
<% end %>
<% if defined? protocol %>
                "protocol" => "<%=protocol%>",
<% end %>
<% if defined? proxy_name %>
                "proxy-name" => "<%=proxy_name%>",
<% end %>
<% if defined? proxy_port %>
                "proxy-port" => <%=proxy_port%>,
<% end %>
<% if defined? redirect_port %>
                "redirect-port" => <%=redirect_port%>,
<% end %>
<% if defined? scheme %>
                "scheme" => "<%=scheme%>",
<% end %>
<% if secure %>
                "secure" => true,
<% end %>
<% if defined? socket_binding %>
                "socket-binding" => "<%=socket_binding%>",
<% end %>

<% if defined? virtual_server %>
                "virtual-server" => "<%=virtual_server%>",
<% end %>

       }
    }
  }
}
